{
  "types": [
    { "id":400, "name":"ChannelSftp", "color":"brown" }
    ],
  "nodes": [
    {
      "name": "SFTP Connect",
      "type": 4,
      "version": 1,
      "import": ["com.jcraft.jsch.ChannelSftp", "com.jcraft.jsch.JSch", "com.jcraft.jsch.JSchException", "com.jcraft.jsch.Session", "com.jcraft.jsch.SftpException"],
      "jar": ["jsch-0.1.55.jar"],
      "java": "@Connect.java",
      "input": [
        { "type": 0, "label": "" },
        { "type": 3, "label": "Host", "value": "localhost", "single_line":true },
        { "type": 1, "label": "Port", "value": 22 },
        { "type": 3, "label": "User", "value": "", "single_line":true },
        { "type": 1, "label": "Auth", "value": 0, "enum": [ "Password", "Public key" ] },
        { "type": 3, "label": "Password", "value": "", "password": true, "single_line":true },
        { "type": 3, "label": "Key", "value": "", "single_line":true }
      ],
      "output": [
        { "type": 0, "label": "Success" },
        { "type": 400, "label": "Channel", "java": {"references": {"type":"ChannelSftp", "name":"channel"}} },
        { "type": 0, "label": "Error" },
        { "type": 3, "label": "Message", "java": {"references": {"type":"String", "name":"msg"}} }
      ]
    },
    {
      "name": "SFTP Disconnect",
      "type": 4,
      "version": 1,
      "import": ["com.jcraft.jsch.ChannelSftp", "com.jcraft.jsch.JSch", "com.jcraft.jsch.JSchException", "com.jcraft.jsch.Session", "com.jcraft.jsch.SftpException"],
      "jar": ["jsch-0.1.55.jar"],
      "java": "try { in{1}.disconnect(); in{1}.getSession().disconnect(); } catch(JSchException e) { }",
      "input": [
        { "type": 0, "label": "" },
        { "type": 400, "label": "Channel" }
      ],
      "output": [
        { "type": 0, "label": "" },
      ]
    },
    {
      "name": "SFTP Put",
      "type": 4,
      "version": 1,
      "import": ["com.jcraft.jsch.ChannelSftp", "com.jcraft.jsch.JSch", "com.jcraft.jsch.JSchException", "com.jcraft.jsch.Session", "com.jcraft.jsch.SftpException"],
      "jar": ["jsch-0.1.55.jar"],
      "java": "@Put.java",
      "input": [
        { "type": 0, "label": "" },
        { "type": 400, "label": "Channel" },
        { "type": 3, "label": "Local file", "value": "", "single_line":true },
        { "type": 3, "label": "Remote file", "value": "", "single_line":true }
      ],
      "output": [
        { "type": 0, "label": "Success" },
        { "type": 400, "label": "Channel", "java": {"references": {"input":1}} },
        { "type": 0, "label": "Error" },
        { "type": 3, "label": "Message", "java": {"references": {"type":"String", "name":"msg"}} }
      ]
    },
    {
      "name": "SFTP Rename",
      "type": 4,
      "version": 1,
      "import": ["com.jcraft.jsch.ChannelSftp", "com.jcraft.jsch.JSch", "com.jcraft.jsch.JSchException", "com.jcraft.jsch.Session", "com.jcraft.jsch.SftpException"],
      "jar": ["jsch-0.1.55.jar"],
      "java": "boolean _{node.id}_renameOK = true; try { in{1}.rename(in{2}, in{3}); } catch(SftpException e) { out{3} = \"SFTP Rename: \"+e.getMessage(); _{node.id}_renameOK = false; } if (_{node.id}_renameOK) { exec{0} } else { exec{2} }",
      "input": [
        { "type": 0, "label": "" },
        { "type": 400, "label": "Channel" },
        { "type": 3, "label": "Current name", "value": "", "single_line":true },
        { "type": 3, "label": "New name", "value": "", "single_line":true }
      ],
      "output": [
        { "type": 0, "label": "Success" },
        { "type": 400, "label": "Channel", "java": {"references": {"input":1}} },
        { "type": 0, "label": "Error" },
        { "type": 3, "label": "Message", "java": {"references": {"type":"String", "name":"msg"}} }
      ]
    }
  ]
}

